{
  "name": "project-eleventy",
  "version": "1.0.0",
  "description": "Starter Project para crear SSGs con 11ty y desplegarlos en Github Pages",
  "main": ".eleventy.js",
  "scripts": {
    "install": "npm install @11ty/eleventy --save-dev",
    "start": "npx @11ty/eleventy --serve --port=5555",
    "clean": "rm -rf docs",
    "deploy": "npx @11ty/eleventy --pathprefix=/",
    "deploy-subdomain": "npx @11ty/eleventy --pathprefix=/subdirectory/"
  },
